{
  "name": "@pppp606/ink-chart",
  "version": "0.2.3",
  "description": "Small visualization components for ink CLI framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "ink-chart-demo": "./bin/demo.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "config": {
    "audit-level": "high"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.bin.json && cp build/bin/demo.js bin/demo.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . && npm run lint:secrets",
    "lint:secrets": "secretlint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w .",
    "test": "jest --passWithNoTests --testPathIgnorePatterns='__tests__/e2e/'",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' jest --passWithNoTests --testPathPattern='__tests__/e2e/'",
    "test:all": "npm run test && npm run test:e2e",
    "test:coverage": "jest --coverage --testPathIgnorePatterns='__tests__/e2e/'",
    "demo": "tsx bin/demo.tsx",
    "audit": "npm audit --audit-level high",
    "audit:fix": "npm audit fix --dry-run",
    "license-check": "npx license-checker --summary",
    "validate:sha-pinning": "node scripts/validate-sha-pinning.js",
    "validate:permissions": "node scripts/validate-permissions.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ink",
    "cli",
    "chart",
    "sparkline",
    "barchart",
    "visualization",
    "react",
    "terminal",
    "secure",
    "typescript",
    "provenance"
  ],
  "author": {
    "name": "Takuto Yuki",
    "email": "takuto.yuki@example.com",
    "url": "https://github.com/pppp606/ink-chart"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pppp606/ink-chart.git"
  },
  "bugs": {
    "url": "https://github.com/pppp606/ink-chart/issues"
  },
  "homepage": "https://github.com/pppp606/ink-chart#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/pppp606"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "tag": "latest",
    "provenance": true
  },
  "peerDependencies": {
    "ink": ">=4",
    "react": ">=18"
  },
  "devDependencies": {
    "@secretlint/secretlint-rule-no-homedir": "^11.2.4",
    "@secretlint/secretlint-rule-preset-recommend": "^11.2.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.3.7",
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.33.2",
    "ink": "^4.4.1",
    "ink-testing-library": "^4.0.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "license-checker": "^25.0.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "secretlint": "^11.2.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
